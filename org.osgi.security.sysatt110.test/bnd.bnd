#
#  OSGi TEST BUNDLE 
#

Bundle-Version:1.0.0.${tstamp}
Bundle-Description: Sysatt110 test bundle

Test-Cases: org.osgi.security.sysatt110.test.Activator

-includeresource: {readme.md}

-buildpath: \
	osgi.enroute.base.api;version=1.0,\
	osgi.enroute.junit.wrapper;version=4.12,\
	osgi.enroute.hamcrest.wrapper

-runbundles: \
	org.apache.felix.configadmin;version='[1.8.6,1.8.7)',\
	org.apache.felix.log;version='[1.0.1,1.0.2)',\
	org.apache.felix.scr;version='[2.0.0,2.0.1)',\
	org.eclipse.equinox.metatype;version='[1.4.100,1.4.101)',\
	org.osgi.security.server.provider;version=snapshot,\
	org.osgi.security.sysatt110.test;version=snapshot,\
	org.osgi.security.util.api;version=snapshot,\
	org.osgi.service.metatype;version='[1.3.0,1.3.1)',\
	osgi.enroute.hamcrest.wrapper;version='[1.3.0,1.3.1)',\
	osgi.enroute.junit.wrapper;version='[4.12.0,4.12.1)'
	
-runrequires: \
	osgi.identity;filter:='(osgi.identity=org.osgi.security.server.provider)',\
	osgi.identity;filter:='(osgi.identity=org.osgi.security.sysatt110.test)'

Bundle-Activator: org.osgi.security.sysatt110.test.Activator

Private-Package: org.osgi.security.sysatt110.test

-dsannotations: *